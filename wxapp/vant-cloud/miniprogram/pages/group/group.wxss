.wrapper {
    width: 100%;
    height: 100%;
    background-color: #f2f2f2;
}
.block {
    width: 100%;
    height: 30rpx;
    background-color: #f2f2f2;
}
.allgroup {
    width: 690rpx;
    margin-left: 30rpx;
    background-color: #ffffff;
    border-radius: 20rpx;
}
.mine-group {
    height: 120rpx;
    /* border-bottom: 1px solid #999999; */

}
.group-pic {
    width: 50rpx;
